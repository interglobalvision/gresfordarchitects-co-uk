@font-face {
    font-family: 'fontello';
    src: url('http://www.gresfordarchitects.co.uk/wp-content/themes/ma-ga-dev/fonts/fontello.eot?96311109');
    src: url('http://www.gresfordarchitects.co.uk/wp-content/themes/ma-ga-dev/fonts/fontello.eot?96311109#iefix') format('embedded-opentype'),
         url('http://www.gresfordarchitects.co.uk/wp-content/themes/ma-ga-dev/fonts/fontello.woff?96311109') format('woff'),
         url('http://www.gresfordarchitects.co.uk/wp-content/themes/ma-ga-dev/fonts/fontello.ttf?96311109') format('truetype'),
         url('http://www.gresfordarchitects.co.uk/wp-content/themes/ma-ga-dev/fonts/fontello.svg?96311109#fontello') format('svg');
    font-weight: normal;
    font-style: normal;

}

[class^="icon-"]:before, [class*=" icon-"]:before {
  font-family: "fontello";
  font-style: normal;
  font-weight: normal;
  speak: none;

  display: inline-block;
  text-decoration: inherit;
  width: 1em;
  margin-right: .2em;
  text-align: center;
  /* opacity: .8; */

  /* For safety - reset parent styles, that can break glyph codes*/
  font-variant: normal;
  text-transform: none;

  /* fix buttons height, for twitter bootstrap */
  line-height: 1em;

  /* Animation center compensation - margins should be symmetric */
  /* remove if not needed */
  margin-left: .2em;

  /* you can be more comfortable with increased icons size */
  /* font-size: 120%; */

  /* Uncomment for 3D effect */
  /* text-shadow: 1px 1px 1px rgba(127, 127, 127, 0.3); */
}

.icon-spotify-circled:before { content: '\e800'; } /* '' */
.icon-linkedin-circled:before { content: '\e801'; } /* '' */
.icon-mail:before { content: '\e802'; } /* '' */
.icon-info-circled:before { content: '\e803'; } /* '' */
.icon-export:before { content: '\e804'; } /* '' */
.icon-cancel-circled:before { content: '\e805'; } /* '' */